(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Bool)
(declare-fun d () Int)
(declare-fun e () Int)
(declare-fun f () Int)
(declare-fun g () Int)
(assert (<= 0 a 18))
(assert (<= 0 e 1))
(assert (<= 0 f 1 b 32 (* d g)))
(assert (= (- d a b) 0 (- a 1 (* 3 f))))
(assert (or c (< a (+ e (* 3 f)))))
(check-sat)
(get-model)
